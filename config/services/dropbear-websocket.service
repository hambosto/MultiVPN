[Unit]
Description=MultiVPN - Elevate Your Connectivity with Secure VPN Solutions
Documentation=https://github.com/hambosto/MultiVPN
After=network.target nss-lookup.target

[Service]
Type=simple
User=root
CapabilityBoundingSet=CAP_NET_ADMIN CAP_NET_BIND_SERVICE
AmbientCapabilities=CAP_NET_ADMIN CAP_NET_BIND_SERVICE
NoNewPrivileges=true
ExecStart=/usr/bin/node --expose-gc /usr/local/bin/proxy3.js -dport 89 -mport 8888
Restart=on-failure

[Install]
WantedBy=multi-user.target
